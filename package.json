    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "babelMacros": {
    "twin": {
      "preset": "emotion"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
